{
  "name": "ToDo",
  "description": "A simple To Do List to keep track of things that need to be done and tick them off as you complete your tasks.",
  "version": "1.2.3",
  "author": {
    "name": "Martin Wirth",
    "email": "mwirth@dizmo.com"
  },
  "contributors": [
    {
      "name": "Grace DizmoGen Converter",
      "email": "support@dizmo.com"
    }
  ],
  "dizmo": {
    "settings": {
      "attributes": {
        "settings/usercontrols/allowresize": true
      },
      "bundle-identifier": "com.dizmo.todo",
      "bundle-name": "ToDo",
      "category": "productivity",
      "height": 280,
      "width": 240,
      "tags": [
        "Todo",
        "tasks"
      ],
      "display-name": "ToDo",
      "elements-version": "1.0",
      "api-version": "1.4",
      "change-log": "Initial version"
    },
    "store": {
      "host": "https://store-api.dizmo.com"
    },
    "build": {
      "minify": {
        "markup": false,
        "scripts": false,
        "styles": false
      }
    }
  },
  "license": "ISC",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:dizmo/Todo.git"
  },
  "scripts": {
    "build": "node ./gulp/tools/run-task.js",
    "clean": "node ./gulp/tools/run-task.js clean",
    "deploy": "node ./gulp/tools/run-task.js deploy",
    "docs": "node ./gulp/tools/run-task.js docs",
    "lint": "node ./gulp/tools/run-task.js lint",
    "prebuild": "node ./gulp/tools/run-task.js prebuild",
    "test": "node ./gulp/tools/run-task.js test",
    "upload": "node ./gulp/tools/run-task.js upload",
    "watch": "node ./gulp/tools/run-task.js watch"
  },
  "dependencies": {
    "@dizmo/dizmo.js": "^1.4.74",
    "core-js": "^3.25.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "ansi-colors": "^4.1.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-copy": "^4.0.1",
    "gulp-dart-sass": "^1.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plist": "^0.9.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-zip": "^5.1.0",
    "rimraf": "^3.0.2",
    "sass": "^1.54.6",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-stream": "^7.0.0",
    "yargs": "^17.5.1"
  },
  "optionalDependencies": {
    "chai": "^4.3.6",
    "chai-spies": "^1.0.0",
    "ignore-styles": "^5.0.1",
    "javascript-obfuscator": "^4.0.0",
    "jsdoc": "^3.6.11",
    "jsdom": "^20.0.0",
    "jsdom-global": "^3.0.2",
    "minami": "^1.2.3",
    "mocha": "^10.0.0",
    "pump": "^3.0.0",
    "request": "^2.88.2",
    "webpack-cli": "^4.10.0",
    "webpack-obfuscator": "^3.5.1"
  }
}
