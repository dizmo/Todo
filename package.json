{
  "name": "ToDo",
  "description": "A simple To Do List to keep track of things that need to be done and tick them off as you complete your tasks.",
  "version": "1.2.3",
  "author": {
    "name": "Martin Wirth",
    "email": "mwirth@dizmo.com"
  },
  "contributors": [
    {
      "name": "Grace DizmoGen Converter",
      "email": "support@dizmo.com"
    }
  ],
  "dizmo": {
    "settings": {
      "attributes": {
        "settings/usercontrols/allowresize": true
      },
      "bundle-identifier": "com.dizmo.todo",
      "bundle-name": "ToDo",
      "category": "productivity",
      "height": 280,
      "width": 240,
      "tags": [
        "Todo",
        "tasks"
      ],
      "display-name": "ToDo",
      "elements-version": "1.0",
      "api-version": "1.3",
      "change-log": "Initial version"
    },
    "store": {
      "host": "https://store-api.dizmo.com"
    },
    "build": {
      "minify": {
        "markup": false,
        "scripts": false,
        "styles": false
      }
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "ansi-colors": "^4.1.1",
    "babel-loader": "^8.1.0",
    "eslint": "^7.2.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-copy": "^4.0.1",
    "gulp-eslint": "^6.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plist": "^0.9.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-ver": "^0.1.0",
    "gulp-zip": "^5.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "webpack-stream": "^5.2.1"
  },
  "license": "ISC",
  "optionalDependencies": {
    "closure-webpack-plugin": "^2.3.0",
    "google-closure-compiler": "^20200517.0.0",
    "jsdoc": "^3.6.4",
    "minami": "^1.2.3",
    "pump": "^3.0.0",
    "webpack-obfuscator": "^1.1.0"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:dizmo/Todo.git"
  },
  "scripts": {
    "build": "node ./gulp/tools/run-task.js",
    "clean": "node ./gulp/tools/run-task.js clean",
    "deploy": "node ./gulp/tools/run-task.js deploy",
    "docs": "node ./gulp/tools/run-task.js docs",
    "lint": "node ./gulp/tools/run-task.js lint",
    "test": "exit 0",
    "upload": "node ./gulp/tools/run-task.js upload",
    "watch": "node ./gulp/tools/run-task.js watch"
  }
}
